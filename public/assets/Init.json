	"AllowSelfRegistration":false,

	"StackedColumnCharts": {
		"chart": {
			"type": "column",
			"height": "60%"
		},
		"colors":["#7cb5ec","#434348"],
		"credits": {
			"enabled": false
		},
		"title": {
			"align": "left",
			"useHTML": true,
			"text": "<font style=\"font-size:30px\">Production monthly results</font>"
		},
		"xAxis": {
			"labels": {
				"style": {
					"fontSize": "30px"
				}
			},
			"categories": ["Tractor assembly", "MQ line", "Backflashed", "Shipped"]
		},
		"yAxis": {
			"min": 0,
			"title": {
				"text": "Quantity"
			},
			"stackLabels": {
				"enabled": false
			}
		},
		"legend": {
			"align": "left",
			"x": 80,
			"verticalAlign": "top",
			"backgroundColor": "white",
			"shadow": false,
			"itemStyle": {
				"fontSize": "30px"
			}
		},
		"plotOptions": {
			"column": {
				"stacking": "normal",
				"dataLabels": {
					"enabled": true,
					"color": "black",
					"style": {
						"fontSize": "30px",
						"color": "black"
					}
				}
			}
		},
		"series": [
			{
				"name": " Not produced"
			},
			{
				"name": " Produced"
			}
		]
	},
	
	"ProductionTargetPieChart": {
		"chart": {
			"type": "pie",
			"height": "100%"
		},
		"colors":["#434348","#7cb5ec"],
		"credits": {
			"enabled": false
		},
		"title": {
			"align": "center",
			"useHTML": true,
			"text": "<font style=\"font-size:30px\">Production Target</font>"
		},
		"plotOptions": {
			"pie": {
				"size": "70%",
				"dataLabels": {
					"enabled": true,
					"crop": false,
					"overflow": "none",
					"format": "{point.y}-{point.percentage:.1f}%",
					"style": {
						"fontSize": "30px",
						"color": "black"
					}
				}
			}
		},
		"series": [
			{
				"data": [
					{
						"name": "Produced",
						"y": "this.props.counts.PROD"
					},
					{
						"name": "Not produced",
						"y": "'this.props.target.PROD_TARGET - this.props.counts.PROD'",
						"sliced": true
					}
				]
			}
		]
	},

	"ShippedTargetPieChart": {
		"chart": {
			"type": "pie",
			"height": "100%"
		},
		"colors":["#434348","#7cb5ec"],
		"credits": {
			"enabled": false
		},
		"title": {
