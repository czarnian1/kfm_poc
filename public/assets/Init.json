					}
				}
			}
		},
		"series": [
			{
				"data": [
					{
						"name": "Shipped",
						"y": "this.props.counts.SHIP"
					},
					{
						"name": "Not shipped",
						"y": "'this.props.target.SHIP_TARGET - this.props.counts.SHIP'",
